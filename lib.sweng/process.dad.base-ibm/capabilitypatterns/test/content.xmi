  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-TDFoVq50HZ7tYkysl5xSlA" name="test_log,_b8AAOmnnEd-4LqPkL9-h8Q" guid="-TDFoVq50HZ7tYkysl5xSlA">
