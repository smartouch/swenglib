  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-qR-Afc8lQkqZiwQylAdS4w" name="stakeholder_request.add,_ZV1JcQNHEeKgIrW2BoUv0Q" guid="-qR-Afc8lQkqZiwQylAdS4w">
