  </org.eclipse.epf.uma:DescriptorDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-oeeEWIGQryanBfhZFE9f8w" name="developer_test,_Z3NvQfLgEeGcxNgLB-1TgQ" guid="-oeeEWIGQryanBfhZFE9f8w">
