  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-uZGZqr3RP71ISqsvdt5IGg" name="developer_test,_ZhhcEQNHEeKgIrW2BoUv0Q" guid="-uZGZqr3RP71ISqsvdt5IGg">
