  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-dT0q4WWIPzABJLIN404TqQ" name="implementation,_Z3OWUfLgEeGcxNgLB-1TgQ" guid="-dT0q4WWIPzABJLIN404TqQ">
    <refinedDescription>&lt;p>&#xD;
    This artifact is the collection of one or more of these elements:&#xD;
&lt;/p>&#xD;
&lt;ul>&#xD;
    &lt;li>&#xD;
        Source code files&#xD;
    &lt;/li>&#xD;
