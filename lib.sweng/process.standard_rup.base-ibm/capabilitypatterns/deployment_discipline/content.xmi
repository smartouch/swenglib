  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-Dv-Ed62kIdwcTO_QuwYcsw" name="assemble_deployment_packages,_RO7QYI-tEd-CJf5elV4f8Q" guid="-Dv-Ed62kIdwcTO_QuwYcsw">
