  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-r2LQe4GQh5Mes2HS71uRWg" name="train_the_trainers,_slxGACyiEd-rIPCOaPACIg" guid="-r2LQe4GQh5Mes2HS71uRWg">
