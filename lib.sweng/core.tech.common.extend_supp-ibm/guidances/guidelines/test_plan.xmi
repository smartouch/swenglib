                    &amp;nbsp;&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; headers=&quot;col2&quot; width=&quot;20%&quot;>&#xD;
                    X&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; headers=&quot;col2&quot; width=&quot;20%&quot;>&#xD;
                    X&#xD;
                &lt;/td>&#xD;
            &lt;/tr>&#xD;
            &lt;tr>&#xD;
                &lt;td headers=&quot;col1&quot; width=&quot;20%&quot;>&#xD;
                    Reliability &#xD;
                    &lt;p>&#xD;
                        (Integrity, Structure)&#xD;
                    &lt;/p>&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; headers=&quot;col2&quot; width=&quot;20%&quot;>&#xD;
