                &lt;td width=&quot;16%&quot;>&#xD;
                    Successful cash withdrawal. Account balance updated to 450.00&#xD;
                &lt;/td>&#xD;
            &lt;/tr>&#xD;
            &lt;tr>&#xD;
                &lt;td align=&quot;right&quot; width=&quot;6%&quot;>&#xD;
                    CW2.&#xD;
                &lt;/td>&#xD;
                &lt;td width=&quot;17%&quot;>&#xD;
                    Scenario 2 - ATM out of Money&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;6%&quot;>&#xD;
                    4987&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;11%&quot;>&#xD;
                    809 - 498&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;11%&quot;>&#xD;
                    100.00&#xD;
