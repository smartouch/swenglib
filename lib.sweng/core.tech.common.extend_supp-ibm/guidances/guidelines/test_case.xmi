                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;6%&quot;>&#xD;
                    49&lt;u>78&lt;/u>&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;11%&quot;>&#xD;
                    809 - 498&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;11%&quot;>&#xD;
                    n/a&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;12%&quot;>&#xD;
                    500.00&#xD;
                &lt;/td>&#xD;
                &lt;td align=&quot;middle&quot; width=&quot;11%&quot;>&#xD;
                    2,000&#xD;
                &lt;/td>&#xD;
                &lt;td width=&quot;16%&quot;>&#xD;
