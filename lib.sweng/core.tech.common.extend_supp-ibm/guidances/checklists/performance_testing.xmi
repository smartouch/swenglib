  <sections xmi:id="_qgh80MUvEdyJCceLNDhCjA" name="All nominal and maximal performance requirements are specified" guid="_qgh80MUvEdyJCceLNDhCjA"/>
  <sections xmi:id="_sK7CEMUvEdyJCceLNDhCjA" name="Performance requirements are reasonable and reflect real constraints in the problem domain" guid="_sK7CEMUvEdyJCceLNDhCjA"/>
  <sections xmi:id="_wG5vcMUvEdyJCceLNDhCjA" name="The specification provides estimates of system performance that indicate which performance requirements, if any, are risks" guid="_wG5vcMUvEdyJCceLNDhCjA"/>
  <sections xmi:id="_ybRhYOCwEdy_TKG9JydNsA" name="The user scenarios and the measurements to be taken are specified" guid="_ybRhYOCwEdy_TKG9JydNsA"/>
  <sections xmi:id="_Fk5doOCxEdy_TKG9JydNsA" name="The playback dynamics, including user think times and input data variations are defined" guid="_Fk5doOCxEdy_TKG9JydNsA"/>
  <sections xmi:id="_z8Bm4MUvEdyJCceLNDhCjA" name="'Bottleneck objects' have been identified and strategies defined to avoid performance bottlenecks" guid="_z8Bm4MUvEdyJCceLNDhCjA"/>
