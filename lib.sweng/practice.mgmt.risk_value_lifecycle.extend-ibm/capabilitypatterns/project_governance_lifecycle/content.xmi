  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-qtwwqqItlUg6UbLq5HFvTg" name="stakeholder_request,_-H034eg5Ed-xk_4F6g-ucw" guid="-qtwwqqItlUg6UbLq5HFvTg">
