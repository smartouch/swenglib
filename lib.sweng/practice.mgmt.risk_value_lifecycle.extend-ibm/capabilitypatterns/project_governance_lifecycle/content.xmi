  <org.eclipse.epf.uma:ActivityDescription xmi:id="-hfjt_vchRCC3iRv4ukrq5g" name="close-out_phase,{99181197-20DC-42DB-B9C4-7B95E015AFA1}" guid="-hfjt_vchRCC3iRv4ukrq5g">
    <mainDescription>&lt;a id=&quot;Description&quot; name=&quot;Description&quot;>&lt;/a> &#xD;
