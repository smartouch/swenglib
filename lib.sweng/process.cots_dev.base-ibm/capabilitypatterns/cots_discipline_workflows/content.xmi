  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-TC_a-z8IYpX4HQ_ioFVUjA" name="review_record,_o2WgJFMREd-b9rMLu3PL1w" guid="-TC_a-z8IYpX4HQ_ioFVUjA">
